nal":{"line":45,"column":17}},{"generated":{"line":62,"column":21},"source":"node_modules/cesium/Source/ThirdParty/GltfPipeline/updateAccessorComponentTypes.js","original":{"line":45,"column":8}},{"generated":{"line":62,"column":24},"source":"node_modules/cesium/Source/ThirdParty/GltfPipeline/updateAccessorComponentTypes.js","original":{"line":45,"column":30}},{"generated":{"line":62,"column":25},"source":"node_modules/cesium/Source/ThirdParty/GltfPipeline/updateAccessorComponentTypes.js","original":{"line":45,"column":8}},{"generated":{"line":63,"column":0},"source":"node_modules/cesium/Source/ThirdParty/GltfPipeline/updateAccessorComponentTypes.js","original":{"line":46,"column":5}},{"name":"updateAccessorComponentTypes","generated":{"line":65,"column":15},"source":"node_modules/cesium/Source/ThirdParty/GltfPipeline/updateAccessorComponentTypes.js","original":{"line":48,"column":19}}],"sources":{"node_modules/cesium/Source/ThirdParty/GltfPipeline/updateAccessorComponentTypes.js":"import addBuffer from './addBuffer.js'\nimport ForEach from './ForEach.js'\nimport readAccessorPacked from './readAccessorPacked.js'\nimport ComponentDatatype from '../../Core/ComponentDatatype.js'\nimport WebGLConstants from '../../Core/WebGLConstants.js'\n\n    /**\n     * Update accessors referenced by JOINTS_0 and WEIGHTS_0 attributes to use correct component types.\n     *\n     * @param {Object} gltf A javascript object containing a glTF asset.\n     * @returns {Object} The glTF asset with compressed meshes.\n     *\n     * @private\n     */\n    function updateAccessorComponentTypes(gltf) {\n        var componentType;\n        ForEach.accessorWithSemantic(gltf, 'JOINTS_0', function(accessorId) {\n            var accessor = gltf.accessors[accessorId];\n            componentType = accessor.componentType;\n            if (componentType === WebGLConstants.BYTE) {\n                convertType(gltf, accessor, ComponentDatatype.UNSIGNED_BYTE);\n            } else if (componentType !== WebGLConstants.UNSIGNED_BYTE\n                    && componentType !== WebGLConstants.UNSIGNED_SHORT) {\n                convertType(gltf, accessor, ComponentDatatype.UNSIGNED_SHORT);\n            }\n        });\n        ForEach.accessorWithSemantic(gltf, 'WEIGHTS_0', function(accessorId) {\n            var accessor = gltf.accessors[accessorId];\n            componentType = accessor.componentType;\n            if (componentType === WebGLConstants.BYTE) {\n                convertType(gltf, accessor, ComponentDatatype.UNSIGNED_BYTE);\n            } else if (componentType === WebGLConstants.SHORT) {\n                convertType(gltf, accessor, ComponentDatatype.UNSIGNED_SHORT);\n            }\n        });\n\n        return gltf;\n    }\n\n    function convertType(gltf, accessor, updatedComponentType) {\n        var typedArray = ComponentDatatype.createTypedArray(updatedComponentType, readAccessorPacked(gltf, accessor));\n        var newBuffer = new Uint8Array(typedArray.buffer);\n        accessor.bufferView = addBuffer(gltf, newBuffer);\n        accessor.componentType = updatedComponentType;\n        accessor.byteOffset = 0;\n    }\n\n    export default updateAccessorComponentTypes;\n"},"lineCount":null}},"error":null,"hash":"486a7d8b8af0ce96dfe5c86991c4f9a8","cacheData":{"env":{}}}